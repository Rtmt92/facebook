  "item": [
    {
      "name": "Users",
      "item": [
        {
          "name": "Create User",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstname\": \"Ervin\",\n  \"lastname\": \"Goby\",\n  \"email\": \"ervin@example.com\",\n  \"age\": 20,\n  \"city\": \"Paris\"\n}"
            },
            "url": { "raw": "http://localhost:3000/user", "protocol": "http", "host": ["localhost"], "port": "3000", "path": ["user"] }
          }
        },
        {
          "name": "Get All Users",
          "request": {
            "method": "GET",
            "url": { "raw": "http://localhost:3000/users", "protocol": "http", "host": ["localhost"], "port": "3000", "path": ["user"] }
          }
        },
        {
          "name": "Get User by ID",
          "request": {
            "method": "GET",
            "url": { "raw": "http://localhost:3000/user/:id", "protocol": "http", "host": ["localhost"], "port": "3000", "path": ["user", ":id"] }
          }
        },
        {
          "name": "Delete User by ID",
          "request": {
            "method": "DELETE",
            "url": { "raw": "http://localhost:3000/user/:id", "protocol": "http", "host": ["localhost"], "port": "3000", "path": ["user", ":id"] }
          }
        }
      ]
    },
    {
